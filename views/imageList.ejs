<html>
	<head>

		<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />	
	</head>
	<body>
		<nav class="navbar">
			
		</nav>
		<div class="container">
			<section id="section1" class="panel panel-default">
				<div class="panel-heading">
					<div class="container">
						<h3 class="panel-title"><strong> <%=bucket%> Amazon S3</strong></h3>
					</div>
				</div>
				<div class="panel-body"> 	
					
						<table class="table">							
		<tr>

			<th>Image</th>
			<th>Name</th>
			<th>Modified</th>
			<th></th>
			
		</tr>
		
							        <% for(var i = 0; i < fileList.length; i++) { %>
        <tr data-id="<%= fileList[i].Key %>">
            <td style="vertical-align: middle;"><img height="100" src="https://s3-us-west-2.amazonaws.com/<%=bucket%>/<%= fileList[i].Key %>" /></td>
            <td style="vertical-align: middle;"> <%= fileList[i].Key %> </td>
            <td style="vertical-align: middle;"> <%= fileList[i].LastModified %> </td>
			<td style="vertical-align: middle;"> <button class="btn btn-primary btn-sm" onClick="window.location.href='/editImage/<%=fileList[i].Key%>'">edit</button> </td>
			<td style="vertical-align: middle;"><button class="btn btn-primary btn-sm" onClick="window.location.href='/delete/<%=fileList[i].Key%>'">delete</button</td>
        </tr>
        <% } %>
							
			
							
						</table>
					
				
		
				<div class="container">
					<div class="col-xs-2">
						<button type="button" class="btn  btn-primary btn-block" onClick="window.location.href='/'">Return</button>
					</div>
					<div class="col-xs-2">
						<button type="button" class="btn  btn-primary btn-block" onClick="window.location.href='/'">Home Page</button>
					</div>
				</div>
				</div>
	  			<footer class="panel-footer">
				
				</footer>
			</section>
		</div>		
	</body>
</html>
